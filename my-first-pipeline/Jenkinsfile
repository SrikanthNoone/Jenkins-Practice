pipeline{
 agent none
    stages {
      stage('front end'){
        agent{
          docker { image 'python:latest' }
          }
        steps { 
          sh 'python --version'
        }
      }
      stage('back end'){
        agent{
          docker {image 'mongo:8.0.0-rc9-jammy'}
        }
      steps{
          sh 'mongo --version'
        }
      }
  }
}
